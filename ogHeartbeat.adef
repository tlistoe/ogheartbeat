sandboxed: false
version: 1.0.0
start: manual
maxFileSystemBytes: 1000K


executables:
{
    ogHeartbeat = ( ogHeartbeatComponent )
}

processes:
{
    envVars:
    {
        LE_LOG_LEVEL = INFO
    }
    run:
    {
        ( ogHeartbeat )
    }
    faultAction: restart
}

bindings:
{
    ogHeartbeat.ogHeartbeatComponent.mangoh_button -> gpioService.le_gpioPin25
}
